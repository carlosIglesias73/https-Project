{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://auth-backend-sigma-lac.vercel.app/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/server.js"
    }
  ]
}